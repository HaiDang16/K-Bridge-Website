@using K_Bridge.Models
@{
    ViewData["Title"] = "Tìm kiếm";
    <link href="/css/Newest.css" rel="stylesheet" />
    bool isKeySearchFind = true;
}

<div class="row">
    <div class=" fs-4">
        Kết quả tìm kiếm
    </div>
    <div class="fs-5 d-flex my-2">
        Kết quả cho từ khoá <p class="ms-2">"SearchKey"</p>
